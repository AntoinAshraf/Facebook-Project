
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>Facebook - Home</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="" />
    <meta name="keywords" content="" />
    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <link href="~/lib/font-awesome/css/fontawesome.min.css" rel="stylesheet" />
    <link href="~/lib/slick-carousel/slick.css" rel="stylesheet" />
    <link href="~/lib/slick-carousel/slick-theme.css" rel="stylesheet" />
    <link href="~/css/style.css" rel="stylesheet" />
    <link href="~/css/responsive.css" rel="stylesheet" />
    <link href="~/css/line-awesome-font-awesome.css" rel="stylesheet" />
    <link href="~/css/jquery.mCustomScrollbar.min.css" rel="stylesheet" />
    <link href="~/css/line-awesome.min.css" rel="stylesheet" />
    <link href="~/css/animate.css" rel="stylesheet" />
    <link href="~/lib/font-awesome/css/all.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="~/css/toastr.min.css">
    <link href="~/css/HomeStyle.css" rel="stylesheet" />

</head>


<body>


    <div class="wrapper">



        <header>
            <div class="container">
                <div class="header-data">
                    <div class="logo">
                        <div id="ll">
                            <a href="index" title=""><i class="fab fa-facebook-f fa-lg" style="color:#fff;"></i></a>
                        </div>
                    </div><!--logo end-->
                    <div class="search-bar">
                        <form>
                            <input type="text" name="search" placeholder="Search...">
                            <button type="submit"><i class="fas fa-search"></i></button>
                        </form>
                    </div><!--search-bar end-->
                    <nav>
                        <ul>
                            <li>
                                <a href="index" title="">
                                    @*<span><img src="~/images/icon1.png" alt=""></span>*@
                                    <div><i class="fas fa-home fa-lg"></i></div>
                                    Home
                                </a>
                            </li>
                            <li>
                                <a href="user-profile.html" title="">
                                    @*<span><img src="~/images/icon4.png" alt=""></span>*@
                                    <div><i class="fas fa-user-circle fa-lg"></i></div>
                                    Profile
                                </a>
                            </li>
                        </ul>
                    </nav><!--nav end-->
                    <div class="menu-btn">
                        <a href="#" title=""><i class="fa fa-bars"></i></a>
                    </div><!--menu-btn end-->
                    <div class="user-account">
                        <div class="user-info">
                            <img src="http://via.placeholder.com/30x30" alt="">
                            <a href="#" title="">@*@ViewBag.User*@ @TempData["Users"]</a>
                            @{
                                TempData.Keep("Users"); 
                            }
                            @*<i class="la la-sort-down"></i>*@
                            <i class="fas fa-chevron-circle-down"></i>
                        </div>
                        <div class="user-account-settingss">
                            <ul class="us-links">
                                <li class="text-center">
                                    <a href="/Account/ChangePassword" title="">
                                        <i class="fas fa-user-cog"></i>
                                        Account Settings
                                    </a>
                                </li>
                            </ul>
                            <h3 class="tc"><a href="/Account/Logout" title=""><i class="fas fa-sign-out-alt"></i> Logout</a></h3>
                        </div><!--user-account-settingss end-->
                    </div>
                </div><!--header-data end-->
            </div>
        </header><!--header end-->





        <main>
            <div class="main-section">
                <div class="container">
                    <div class="main-section-data">
                        <div class="row">

                            <div class="col-lg-12 col-md-10 no-pd">
                                <div class="main-ws-sec">
                                    @RenderBody()
                                </div><!--main-ws-sec end-->
                            </div>

                        </div>
                    </div><!-- main-section-data end-->
                </div>
            </div>
        </main>







    </div><!--theme-layout end-->
    @*scripts*@
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/popper.js/popper.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/js/jquery.mCustomScrollbar.js"></script>
    <script src="~/lib/slick-carousel/slick.min.js"></script>
    <script src="~/js/scrollbar.js"></script>
    <script type="text/javascript" src="~/js/toastr.min.js"></script>
    <script src="~/js/script.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>